# Example environment (no secrets)

# Initial WebSocket endpoint shown in the UI (bundled Go server)
NEXT_PUBLIC_WS_URL_DEFAULT=ws://localhost:9999/ws

# Message buffer limit for the in-memory ring buffer
NEXT_PUBLIC_MESSAGE_BUFFER_LIMIT=2000

# Turn on E2E-friendly test attributes in the UI
NEXT_PUBLIC_E2E=false

# Enable Vercel Analytics in the app layout when true/1
NEXT_PUBLIC_ENABLE_ANALYTICS=false
